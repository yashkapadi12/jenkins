pipeline {
    agent any

    stages {
        stage('Code') {
            steps {
                echo 'This is a Code'
            }
        }

        stage('Build') {
            steps {
                echo 'This is a Build'
            }
        }

        stage('Test') {
            steps {
                echo 'This is a test'
            }
        }

        stage('Deploy') {
            steps {
                echo 'This is a deploy'
            }
        }
    }
}
